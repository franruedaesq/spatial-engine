{
  "name": "@spatial-engine/react",
  "version": "0.0.1",
  "description": "React Three Fiber adapter for @spatial-engine/core",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@spatial-engine/core": "workspace:*",
    "@spatial-engine/three": "workspace:*"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "@react-three/fiber": ">=8.0.0",
    "three": ">=0.150.0"
  },
  "devDependencies": {
    "react": "^18.3.1",
    "@types/react": "^18.3.3",
    "three": "^0.166.1",
    "@types/three": "^0.166.0",
    "@react-three/fiber": "^8.17.5"
  }
}
