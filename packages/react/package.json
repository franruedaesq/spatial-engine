{
  "name": "@spatial-engine/react",
  "version": "0.0.3",
  "description": "React Three Fiber adapter for @spatial-engine/core",
  "author": "Francisco Rueda Esquivel",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/franruedaesq/spatial-engine.git",
    "directory": "packages/react"
  },
  "keywords": [
    "react",
    "react-three-fiber",
    "r3f",
    "octree",
    "spatial",
    "aabb",
    "hooks",
    "3d",
    "data-oriented-design",
    "zero-gc"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "@spatial-engine/core": "workspace:*",
    "@spatial-engine/three": "workspace:*"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "@react-three/fiber": ">=8.0.0",
    "three": ">=0.150.0"
  },
  "devDependencies": {
    "react": "^18.3.1",
    "@types/react": "^18.3.3",
    "three": "^0.166.1",
    "@types/three": "^0.166.0",
    "@react-three/fiber": "^8.17.5"
  }
}