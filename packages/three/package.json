{
  "name": "@spatial-engine/three",
  "version": "0.0.2",
  "description": "Three.js adapter for @spatial-engine/core",
  "author": "Francisco Rueda Esquivel",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/franruedaesq/spatial-engine.git",
    "directory": "packages/three"
  },
  "keywords": [
    "three",
    "threejs",
    "octree",
    "spatial",
    "aabb",
    "3d",
    "data-oriented-design",
    "zero-gc"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "@spatial-engine/core": "workspace:*"
  },
  "peerDependencies": {
    "three": ">=0.150.0"
  },
  "devDependencies": {
    "three": "^0.166.1",
    "@types/three": "^0.166.0"
  }
}